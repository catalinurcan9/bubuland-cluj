          "_customHTML": "<section data-bs-version=\"5.1\" class=\"steps1 marketm4_steps1\" group=\"Steps\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n\t\t<header>Show/Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Step Title\" name=\"showStepTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Step Text\" name=\"showStepText\" checked>\n\t\t<input type=\"checkbox\" title=\"Show Button\" name=\"showButtons\" checked>\n\t\t<input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#3c011a\">\n\t\t<select title=\"Steps Count\" name=\"stepsAmount\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\">3</option>\n\t\t\t<option value=\"4\" selected>4</option>\n\t\t\t<option value=\"5\">5</option>\n\t\t\t<option value=\"6\">6</option>\n\t\t\t<option value=\"7\">7</option>\n\t\t</select>\n\t\t<input type=\"range\" inline title=\"Step Size\" name=\"stepSize\" min=\"10\" max=\"30\" step=\"1\" value=\"24\">\n\t\t<select title=\"Show Image\" name=\"addBlockAmount\">\n\t\t\t<option value=\"1\">odd</option>\n\t\t\t<option value=\"2\">even</option>\n\t\t\t<option value=\"3\">all</option>\n\t\t\t<option value=\"4\" selected>none</option>\n\t\t</select>\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background-panda.webp\" selected>\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\">\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.